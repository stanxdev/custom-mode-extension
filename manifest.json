{
    "name": "Custom Mode",
    "description": "Inject JS, CSS. Redirect requests. Modify headers.",
    "version": "2025",
    "manifest_version": 3,
    "icons": {
        "128": "icons/icon.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "permissions": [
        "scripting",
        "storage",
        "declarativeNetRequest"
    ]
}
