{
    "request_rules": [
        {
            "id": "instagram.com => imginn.com",
            "descript": [
                "exception: ! at the end"
            ],
            "action": {
                "type": "redirect",
                "redirect": {
                    "regexSubstitution": "https://imginn.com/p/\\1"
                }
            },
            "condition": {
                "regexFilter": "^https://www\\.instagram.com/(?:.*?/)?(?:reels?|p)/(.*)[^!]$",
                "resourceTypes": [
                    "main_frame"
                ]
            }
        }
    ],
    "content_scripts": [
        {
            "id": "imginn.com",
            "matches": [
                "https://imginn.com/*"
            ],
            "css": [
                "instagram/imginn.com.css"
            ],
            "run_at": "document_start"
        }
    ]
}
